{
  "name": "vue-starter-kit",
  "version": "0.0.1",
  "description": "Starter kit for Vue 2 projects",
  "license": "CC-BY-NC-ND 4.0",
  "author": "Mostafa Elshamy",
  "private": true,
  "main": "server.mjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/melhosseiny/vue-starter-kit.git"
  },
  "scripts": {
    "start": "node server.mjs",
    "start:dev": "NODE_ENV=development node server.mjs",
    "watch": "npx webpack --watch --config webpack.config.dev.js",
    "build:dev": "npx webpack --config webpack.config.dev.js",
    "build": "npx webpack --config webpack.config.prod.js",
    "test": "npx jest",
    "eslint": "npx eslint src/**/*.{js,vue}",
    "stylelint": "npx stylelint src/**/*.{css,vue}"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "vue": "^2.6.12",
    "vue-router": "^3.4.8",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@vue/test-utils": "^1.1.0",
    "autoprefixer": "^10.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.1",
    "babel-loader": "^8.1.0",
    "browserslist": "^4.14.5",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.12.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-vue": "^7.1.0",
    "jest": "^26.6.1",
    "jest-serializer-vue": "^2.0.2",
    "mini-css-extract-plugin": "^1.2.1",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "^6.7.0",
    "raw-loader": "^4.0.2",
    "style-loader": "^2.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.4",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.3.1",
    "webpack-cli": "^4.1.0",
    "webpack-dev-middleware": "^4.0.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.2.0"
  },
  "engines": {
    "node": "14.x"
  },
  "browserslist": {
    "development": [
      "last 1 version",
      "> 1%",
      "ie 11",
      "not dead"
    ],
    "production": [
      "last 1 version",
      "> 1%",
      "not ie 11",
      "not dead"
    ],
    "test": [
      "node 14"
    ]
  }
}
